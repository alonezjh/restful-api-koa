{
  "extends": ["tslint:recommended"],
  "rules": {
    "no-console": false,
    "object-literal-sort-keys": false,
    "member-access": false,
    "no-empty": false,
    "class-name": false,
    "ordered-imports": false,
    "array-type": false,
    "forin": false,
    "curly": false,
    "no-shadowed-variable": false,
    "quotemark": false,
    "arrow-parens": false,
    "no-string-literal": false,
    "object-literal-shorthand": false,
    "variable-name": false
  },
  "linterOptions": {
    "exclude": ["**/*.json", "node_modules"]
  }
}